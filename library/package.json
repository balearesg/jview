{
	"name": "jview",
	"version": "0.0.1",
	"title": "JView Preview",
	"description": "",
	"license": "ISC",
	"layout": "main-jview-layout",
	"template": "template/template.json",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "modules"
	},
	"params": {
		"application": {
			"tables": {
				"rows": 5
			}
		}
	},
	"libraries": {
		"imports": [
			"pragmate-ui"
		]
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"environment": "development",
				"platform": "web",
				"ports": {
					"bundles": 6510
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/events": "^0.0.7",
		"@beyond-js/kernel": "^0.1.9",
		"@beyond-js/react-18-widgets": "^1.0.3",
		"@beyond-js/reactive": "^1.1.6",
		"socket.io-client": "^4.5.4",
		"tippy.js": "^6.3.7",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7"
	}
}
